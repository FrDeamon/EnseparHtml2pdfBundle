{
    "name": "ensepar/html2pdf-bundle",
    "type": "symfony-bundle",
    "description": "Use HTML2PDF as a Symfony2 Service",
    "keywords": ["pdf", "tcpdf", "html2pdf"],
    "homepage": "https://github.com/OwlyCode/EnseparHtml2pdfBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tristan Maindron",
            "email": "tmaindron@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/dependency-injection": "^2.8 || ^3.4 || ^4.1",
        "symfony/http-kernel": "^2.8 || ^3.4 || ^4.1",
        "spipu/html2pdf": "~5.2"
    },
    "require-dev": {
        "symfony/framework-bundle": "^2.8 || ^3.4 || ^4.1",
        "symfony/phpunit-bridge": "^2.8 || ^3.4 || ^4.1",
        "phpunit/phpunit": "^8.2 || ^7.5"
     },
    "autoload": {
        "psr-4": { "Ensepar\\Html2pdfBundle\\": "./" }
    },
    "target-dir": "Ensepar/Html2pdfBundle"
}
